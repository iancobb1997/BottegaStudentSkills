<div class="container">
  <nav class="white">
    <div class="nav-wrapper">
      <form action="" method="get">
        <div class="input-field col s6 s12">
        <div class="row">
          <div class="col s10">
            <div class="input-field col s6 s12">
              <i class="black-text material-icons prefix">search</i>
              <input type="text" placeholder="I'm looking for a programmer in..." id="q" class="autocomplete red-text search-programmer" name="q" value="<%= params[:q] %>" required>
            </div>
          </div>
            <div class=" col s2">
              <%= link_to "Clear", pages_index_path, class: "waves-effect waves-light btn"%>
          </div>
        </div>
      </form>
    </div>
  </nav>
</div>

<div class="row">
  <div class="col s1">
  </div>
  <div class="col s10">
    <% @users.each do |user| %>
      <% unless user.roles == [:admin, :user] %>
        <div class="col s12 m3">
          <div class="card">
            <div class="card-image">
              <%= image_tag 'images.jpeg', class: 'avatar' %>
                <%= link_to user.name, show_path(user), id: "show_#{user.id}", class: "name" %>
            </div>
            <div class="card-content">
              <div class>
              </div>
              <div class="card-action">
                <%= user.skills %>
              </div>
            </div>
      </div>
    </div>
    <% end %>
   <% end %>
  </div>
  <div class="col s1">
  </div>
</div>
